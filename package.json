{
  "name": "anicca",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prebuild": "rimraf .cache && rimraf dist",
    "build": "npm run prebuild && parcel build src/*.html",
    "dev": "parcel src/*.html",
    "clean": "rm -rf dist .parcel-cache",
    "parcel:serve": "parcel serve src/markup/index.pug",
    "parcel:watch": "parcel watch src/markup/index.pug",
    "parcel:open": "parcel src/markup/index.pug --port 9999",
    "start": "npm-run-all -s clean -p parcel:watch parcel:open"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "parcel": "^2.0.1"
  },
  "devDependencies": {
    "@parcel/transformer-pug": "^2.0.1",
    "@parcel/transformer-sass": "^2.0.1",
    "autoprefixer": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "pug": "^3.0.2",
    "tailwindcss": "^3.0.23"
  }
}
